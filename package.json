{
  "name": "image-router-api",
  "version": "1.0.0",
  "description": "API router for image generation services following OpenAI schema",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --config=jest.config.js",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules npx jest --config=jest.config.js --watch"
  },
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.4.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "node-fetch": "^3.3.2",
    "@prisma/client": "^5.10.0",
    "jsonwebtoken": "^9.0.2",
    "https-proxy-agent": "^5.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "prisma": "^5.10.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  },
  "jest": {
    "transform": {},
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
} 