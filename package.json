{
  "name": "image-router-api",
  "version": "1.0.0",
  "description": "API router for image generation services following OpenAI schema",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --detectOpenHandles",
    "lint": "eslint . --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.4.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "node-fetch": "^3.3.2",
    "@prisma/client": "^5.10.0",
    "jsonwebtoken": "^9.0.2",
    "https-proxy-agent": "^5.0.1",
    "jest": "^29.7.0",
    "multer": "2.0.2",
    "google-auth-library": "^9.0.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/lib-storage": "^3.450.0",
    "zod": "^3.23.8",
    "yaml": "^2.2.2",
    "@asteasolutions/zod-to-openapi": "^7.3.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "prisma": "^5.10.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-config-prettier": "^9.0.0",
    "nock": "^13.3.0",
    "aws-sdk-client-mock": "^3.1.0"
